<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aqualala">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name="fr.unilim.aqualala.controller.ModificationEclairage"></activity>
        <activity android:name="fr.unilim.aqualala.controller.Eclairage" />
        <activity android:name="fr.unilim.aqualala.Temperatures" />
        <activity android:name="fr.unilim.aqualala.controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>